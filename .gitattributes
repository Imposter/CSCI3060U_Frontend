*.fsproj text eol=crlf
*.lsproj text eol=crlf
*.wixprojtext eol=crlf
*.modelproj text eol=crlf
*.sqlprojtext eol=crlf
*.wmaprojtext eol=crlf

*.xproj text eol=crlf
